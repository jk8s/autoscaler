---
apiVersion: v1
kind: Secret
metadata:
  name: cluster-autoscaler-cloud-config
  namespace: kube-system
type: Opaque
stringData:
  cloud-config: |-
    [vsphere]
    vsphere-server=VSPHERE_SERVER
    vsphere-username=VSPHERE_USERNAME
    vsphere-password=VSPHERE_PASSWORD
    vsphere-insecure="true"
    vsphere-datacenter="datacenter-jk8s"
    vsphere-resource-pool="jk8s-resource-pool-01"
    vsphere-template="ubuntu-base-18.04.4"